{
  "name": "auth-express-typescript-example",
  "version": "1.0.0",
  "description": "An auth example made with express, mongoose, jwt, typescript and docker",
  "main": "dist",
  "type": "module",
  "scripts": {
    "start": "NODE_ENV=prod node dist",
    "prestart": "pnpm build",
    "dev": "tsc -p tsconfig.json --watch & NODE_ENV=dev nodemon dist",
    "predev": "pnpm build",
    "build": "tsc -p tsconfig.json",
    "clean": "rimraf dist && rm -f tsconfig.tsbuildinfo",
    "lint": "eslint --fix src"
  },
  "keywords": [
    "express",
    "typescript",
    "docker",
    "node"
  ],
  "author": "kytnacode",
  "license": "CC0",
  "devDependencies": {
    "@types/eslint": "^8.44.1",
    "@types/express": "^4.17.17",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "eslint": "^8.45.0",
    "jshint": "^2.13.6",
    "nodemon": "^3.0.1",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "winston": "^3.10.0"
  }
}
