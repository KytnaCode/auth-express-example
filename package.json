{
  "name": "auth-express-typescript-example",
  "version": "1.0.0",
  "description": "An auth example made with express, mongoose, jwt, typescript and docker",
  "main": "dist",
  "type": "module",
  "scripts": {
    "start": "node dist",
    "prestart": "pnpm build",
    "dev": "tsc -p tsconfig.json --watch & nodemon dist",
    "build": "tsc -p tsconfig.json",
    "prebuild": "rimraf dist",
    "lint": "jshint src --fix"
  },
  "keywords": [
    "express",
    "typescript",
    "docker",
    "node"
  ],
  "author": "kytnacode",
  "license": "CC0",
  "devDependencies": {
    "@types/express": "^4.17.17",
    "jshint": "^2.13.6",
    "nodemon": "^3.0.1",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "express": "^4.18.2"
  }
}
